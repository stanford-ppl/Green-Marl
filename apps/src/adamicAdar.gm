
Proc adamicAdar(G:Graph, aa: E_P<Double>)
{
    Foreach (e:G.Edges)
    {
        Node from = e.FromNode();
        Node to = e.ToNode();
       
        e.aa = Sum(n: from.CommonNbrs(to)) {1 / Log(n.NumNbrs())};
    }
}


Proc adamicAdar2(G:Graph, aa: E_P<Double>)
{
    Foreach (e:G.Edges)
    {
        Node from = e.FromNode();
        Node to = e.ToNode();
       
        e.aa = Sum(n: from.Nbrs)(n.IsNbrFrom(to)) {1 / Log(n.NumNbrs())};
    }
}
